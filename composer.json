{
  "name": "znck/plug",
  "type": "library",
  "description": "Addons for Laravel Eloquent",
  "keywords": [
    "znck",
    "laravel",
    "eloquent",
    "database"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rahul Kadyan",
      "email": "hi@znck.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Znck\\Plug\\Eloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Znck\\Tests\\Plug\\Eloquent\\": "tests/"
    }
  },
  "require": {
    "php": "~7.0",
    "illuminate/support": "^5.2",
    "illuminate/database": "^5.2",
    "illuminate/validation": "^5.2",
    "ramsey/uuid": "^3.2"
  },
  "require-dev": {
    "graham-campbell/testbench": "^3.1",
    "phpunit/phpunit": "^5.1",
    "mockery/mockery": "^0.9.4",
    "phpunit/php-code-coverage": "^3.1",
    "satooshi/php-coveralls": "^1.0",
    "fabpot/php-cs-fixer": "^1.11"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
